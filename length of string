#include <iostream>
#include <string>
using namespace std;

int stringLength(string str) {
    int length = 0;
    while (str[length] != '\0')
        length++;
    return length;
}

int main() {
    string str;
    cout << "Enter a string: ";
    getline(cin, str);

    cout << "Length = " << stringLength(str) << endl;
    return 0;
}
